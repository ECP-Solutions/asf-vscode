class Calculator extends BaseCalculator {
    field result = 0;
    # Class constructor
    constructor(initialValue) {
        super();
        this.result = initialValue;
    }
    
    static create() {
        return new Calculator(0);
    }
    
    add(x, y) {
        let total = x + y;
        this.result = total;
        return total;
    }
}

fun fibonacci(n) {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

