{
  "name": "asf-language",
  "displayName": "VBA Advanced Scripting Syntax",
  "description": "Advanced Scripting Framework language support for VS Code",
  "version": "1.0.6",
  "publisher": "ECPSolutions",
  "icon": "icons/asf-icon.png",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [{
      "id": "asf",
      "aliases": ["ASF", "Advanced Scripting Framework"],
      "extensions": [".vas"],
      "configuration": "./language-configuration.json",
      "icon": {
          "light": "./icons/asf-icon.png",
          "dark": "./icons/asf-icon.png"
        }
    }],
    "grammars": [{
      "language": "asf",
      "scopeName": "source.asf",
      "path": "./syntaxes/asf.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "asf",
        "path": "./snippets/asf.json"
      }
    ],
    "configurationDefaults": {
      "[asf]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
      },
      "files.associations": {
        "*.asf": "asf",
        "*.vas": "asf"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ECP-Solutions/asf-vscode"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0"
  }
}

