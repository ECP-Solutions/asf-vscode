{
  "Function Declaration": {
    "prefix": "fun",
    "body": [
      "fun ${1:name}(${2:params}) {",
      "\t$0",
      "};"
    ],
    "description": "Function declaration"
  },
  "Object": {
    "prefix": "obj",
    "body": [
      "{",
      "\t${1:key1}: ${2:value1}",
      "\t${3:key2}: ${4:value2}",
      "};"
    ],
    "description": "Object Literal"
  },
  "Class Declaration": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName} {",
      "\tconstructor(${2:params}) {",
      "\t\t$0",
      "\t}",
      "};"
    ],
    "description": "Class declaration"
  },
  "Class with Extends": {
    "prefix": "classx",
    "body": [
      "class ${1:ClassName} extends ${2:ParentClass} {",
      "\tconstructor(${3:params}) {",
      "\t\tsuper(${4:args});",
      "\t\t$0",
      "\t}",
      "};"
    ],
    "description": "Class declaration with extends"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if (${1:condition}) {",
      "\t$0",
      "};"
    ],
    "description": "If statement"
  },
  "If-Else Statement": {
    "prefix": "ife",
    "body": [
      "if (${1:condition}) {",
      "\t$2",
      "} else {",
      "\t$0",
      "};"
    ],
    "description": "If-else statement"
  },
  "If-ElseIf-Else Statement": {
    "prefix": "ifel",
    "body": [
      "if (${1:condition}) {",
      "\t$2",
      "} elseif (${3:condition}) {",
      "\t$4",
      "} else {",
      "\t$0",
      "};"
    ],
    "description": "If-elseif-else statement"
  },
  "For Loop (C-style)": {
    "prefix": "for",
    "body": [
      "for (${1:i = 0}, ${2:i < n}, ${3:i = i + 1}) {",
      "\t$0",
      "};"
    ],
    "description": "C-style for loop"
  },
  "For-In Loop": {
    "prefix": "forin",
    "body": [
      "for (${1:key} in ${2:object}) {",
      "\t$0",
      "};"
    ],
    "description": "For-in object loop"
  },
  "For-In array": {
    "prefix": "forin",
    "body": [
      "for (${1:key} in ${2:array}) {",
      "\t$0",
      "};"
    ],
    "description": "For-in array loop"
  },
  "For-Of Loop": {
    "prefix": "forof",
    "body": [
      "for (${1:item} of ${2:array}) {",
      "\t$0",
      "};"
    ],
    "description": "For-of array loop"
  },
  "For-Of object": {
    "prefix": "forof",
    "body": [
      "for (${1:item} of ${2:object}) {",
      "\t$0",
      "};"
    ],
    "description": "For-of object loop"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while (${1:condition}) {",
      "\t$0",
      "};"
    ],
    "description": "While loop"
  },
  "Switch Statement": {
    "prefix": "switch",
    "body": [
      "switch (${1:expression}) {",
      "\tcase ${2:value}{",
      "\t\t$3",
      "\t}",
      "\tdefault{",
      "\t\t$0",
      "\t}",
      "};"
    ],
    "description": "Switch statement"
  },
  "Try-Catch": {
    "prefix": "try",
    "body": [
      "try {",
      "\t$1",
      "} catch (${2:error}) {",
      "\t$0",
      "};"
    ],
    "description": "Try-catch block"
  },
  "Import Default": {
    "prefix": "imp",
    "body": [
      "import ${1:name} from \"${2:module}\";"
    ],
    "description": "Import default"
  },
  "Import Named": {
    "prefix": "impn",
    "body": [
      "import { ${1:name} } from \"${2:module}\";"
    ],
    "description": "Import named"
  },
  "Import Namespace": {
    "prefix": "impns",
    "body": [
      "import * as ${1:name} from \"${2:module}\";"
    ],
    "description": "Import namespace"
  },
  "Export Named": {
    "prefix": "exp",
    "body": [
      "export { ${1:name}, ${2:name2} };"
    ],
    "description": "Export named"
  },
  "Export Default": {
    "prefix": "expd",
    "body": [
      "export default ${1:value};"
    ],
    "description": "Export default"
  },
  "Array Destructuring": {
    "prefix": "dest",
    "body": [
      "[${1:a}, ${2:b}] = ${3:array};"
    ],
    "description": "Array destructuring"
  },
  "Array Destructuring with Rest": {
    "prefix": "destr",
    "body": [
      "[${1:first}, ...${2:rest}] = ${3:array};"
    ],
    "description": "Array destructuring with rest"
  },
  "Print": {
    "prefix": "print",
    "body": [
      "print(${1:message});"
    ],
    "description": "Print statement"
  },
  "Let Declaration": {
    "prefix": "let",
    "body": [
      "let ${1:name} = ${2:value};"
    ],
    "description": "Let declaration"
  },
  "Arrow Function": {
    "prefix": "afun",
    "body": [
      "fun(${1:params}) { ${2:body} };"
    ],
    "description": "Anonymous function"
  },
  "Template Literal": {
    "prefix": "tpl",
    "body": [
      "`${${1:expression}}`"
    ],
    "description": "Template literal"
  },
  "Prototype Method Definition": {
    "prefix": "prototype",
    "body": [
      "prototype.COM.${1:ClassName} ${2:methodName}($3) {",
      "\t${4:// Method implementation}",
      "\t${5:// 'this' refers to the COM object instance}",
      "\treturn ${6:this}; ${7:// Enable method chaining}",
      "};"
    ],
    "description": "Define a COM prototype method for monkey patching"
  },
  "Prototype Range Method": {
    "prefix": "protorange",
    "body": [
      "prototype.COM.Range ${1:methodName}($2) {",
      "\t${3:// Access range properties via 'this'}",
      "\t${4:// this.Value, this.Address, this.Cells, etc.}",
      "\treturn this; // Enable chaining",
      "};"
    ],
    "description": "Define a Range prototype method"
  },
  "Prototype ListRow Method": {
    "prefix": "protolistrow", 
    "body": [
      "prototype.COM.ListRow ${1:methodName}($2) {",
      "\tlet headers = this.parent.listcolumns;",
      "\tlet values = this.range.value2;",
      "\t${3:// Process row data}",
      "\treturn ${4:this};",
      "};"
    ],
    "description": "Define a ListRow prototype method for table processing"
  },
  "Prototype Worksheet Method": {
    "prefix": "protoworksheet",
    "body": [
      "prototype.COM.Worksheet ${1:methodName}($2) {",
      "\t${3:// Access worksheet properties via 'this'}",
      "\t${4:// this.Name, this.Range, this.Cells, etc.}",
      "\treturn this;",
      "};"
    ],
    "description": "Define a Worksheet prototype method"
  },
  "Prototype Document Method": {
    "prefix": "protodocument",
    "body": [
      "prototype.COM.Document ${1:methodName}($2) {",
      "\t${3:// Access Word document via 'this'}",
      "\t${4:// this.Range, this.Paragraphs, this.Tables, etc.}",
      "\treturn this;",
      "};"
    ],
    "description": "Define a Word Document prototype method"
  },
  "Prototype Presentation Method": {
    "prefix": "protopresentation",
    "body": [
      "prototype.COM.Presentation ${1:methodName}($2) {",
      "\t${3:// Access PowerPoint presentation via 'this'}",
      "\t${4:// this.Slides, this.SlideMaster, this.Name, etc.}",
      "\treturn this;",
      "};"
    ],
    "description": "Define a PowerPoint Presentation prototype method"
  },
  "Prototype Slide Method": {
    "prefix": "protoslide",
    "body": [
      "prototype.COM.Slide ${1:methodName}($2) {",
      "\t${3:// Access slide properties via 'this'}",
      "\t${4:// this.Shapes, this.Layout, this.Background, etc.}",
      "\treturn this;",
      "};"
    ],
    "description": "Define a PowerPoint Slide prototype method"
  },
  "Prototype Recordset Method": {
    "prefix": "protorecordset",
    "body": [
      "prototype.COM.Recordset ${1:methodName}($2) {",
      "\t${3:// Access Access recordset via 'this'}",
      "\t${4:// this.Fields, this.RecordCount, this.MoveNext, etc.}",
      "\treturn this;",
      "};"
    ],
    "description": "Define an Access Recordset prototype method"
  },
  "Prototype MailItem Method": {
    "prefix": "protomailitem",
    "body": [
      "prototype.COM.MailItem ${1:methodName}($2) {",
      "\t${3:// Access Outlook mail item via 'this'}",
      "\t${4:// this.Subject, this.Body, this.Recipients, etc.}",
      "\treturn this;",
      "};"
    ],
    "description": "Define an Outlook MailItem prototype method"
  },
  "This Property Access": {
    "prefix": "this",
    "body": "this.${1:property}",
    "description": "Access COM object property via 'this'"
  },
  "VBA Escape": {
    "prefix": "vba",
    "body": [
      "@(${1:VBA_CODE})"
    ],
    "description": "VBA escape hatch"
  }
}
